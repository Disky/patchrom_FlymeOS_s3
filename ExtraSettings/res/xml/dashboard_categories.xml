<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@id/wireless_section" android:title="@string/header_category_wireless_networks">
        <dashboard-tile android:icon="@drawable/ic_settings_nfc" android:id="@id/nfc_settings" android:title="@string/nfc_settings_title" android:fragment="com.mediatek.nfc.NfcSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_hotknot" android:id="@id/hotknot_settings" android:title="@string/hotknot_settings_title" android:fragment="com.mediatek.settings.hotknot.HotKnotSettings" />
        <dashboard-tile android:icon="@drawable/ic_sim_sd" android:id="@id/sim_settings" android:title="@string/sim_settings_title" android:fragment="com.android.extrasettings.sim.SimSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_data_usage" android:id="@id/data_usage_settings" android:title="@string/data_usage_summary_title" android:fragment="com.android.extrasettings.DataUsageSummary" />
        <dashboard-tile android:id="@id/operator_settings" android:fragment="com.android.extrasettings.WirelessSettings">
            <intent android:action="com.android.extrasettings.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/device_section" android:title="@string/header_category_device">
        <dashboard-tile android:icon="@drawable/ic_settings_home" android:id="@id/home_settings" android:title="@string/home_settings" android:fragment="com.android.extrasettings.HomeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/display_settings" android:fragment="com.android.extrasettings.DisplaySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_notifications" android:id="@id/notification_settings" android:title="@string/notification_settings" android:fragment="com.mediatek.audioprofile.AudioProfileSettings" />
        <dashboard-tile android:icon="@drawable/v_smartwake_icon" android:id="@id/smartwake_settings" android:title="@string/smartwake_settings_title">
            <intent android:targetPackage="es.fraggel.gesturecontrol" android:action="android.intent.action.MAIN" android:targetClass="es.fraggel.gesturecontrol.screenOffActivity" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_storage" android:id="@id/storage_settings" android:title="@string/storage_settings" android:fragment="com.android.extrasettings.deviceinfo.Memory" />
        <dashboard-tile android:icon="@drawable/ic_settings_battery" android:id="@id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.android.extrasettings.fuelgauge.PowerUsageSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications" android:id="@id/application_settings" android:title="@string/applications_settings" android:fragment="com.android.extrasettings.applications.ManageApplications" />
        <dashboard-tile android:icon="@drawable/ic_settings_multiuser" android:id="@id/user_settings" android:title="@string/user_settings_title" android:fragment="com.android.extrasettings.users.UserSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_nfc_payment" android:id="@id/nfc_payment_settings" android:title="@string/nfc_payment_settings_title" android:fragment="com.android.extrasettings.nfc.PaymentSettings" />
        <dashboard-tile android:id="@id/manufacturer_settings" android:fragment="com.android.extrasettings.WirelessSettings">
            <intent android:action="com.android.extrasettings.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/header_category_system">
        <dashboard-tile android:icon="@drawable/ic_settings_security" android:id="@id/security_settings" android:title="@string/security_settings_title" android:fragment="com.android.extrasettings.SecuritySettings" />
        <!--<dashboard-tile android:icon="@drawable/ic_settings_performance" android:id="@id/about_settings" android:title="@string/tf_rendimiento">
            <intent android:targetPackage="com.grarak.kerneladiutor" android:action="android.intent.action.MAIN" android:targetClass="com.grarak.kerneladiutor.MainActivity" />
        </dashboard-tile>-->
        <dashboard-tile android:icon="@drawable/ic_settings_schpwronoff" android:id="@id/power_settings" android:title="@string/schedule_power_on_off_settings_title">
            <intent android:action="com.android.extrasettings.SCHEDULE_POWER_ON_OFF_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.extrasettings.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_supersu" android:id="@id/supersu_settings" android:title="@string/supersu_title">
            <intent android:targetPackage="eu.chainfire.supersu" android:action="android.intent.action.MAIN" android:targetClass="eu.chainfire.supersu.MainActivity" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.extrasettings.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>